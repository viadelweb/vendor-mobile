{
  "expo": {
    "name": "vendor-mobile",
    "slug": "vendor-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "viadelweb+vendermobile",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.viadelweb.vendermobile",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "config": {
        "googleMapsApiKey": "<apiKey>"
      }
    },
    "android": {
      "package": "com.viadelweb.vendermobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": { "apiKey": "<apiKey>" }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
